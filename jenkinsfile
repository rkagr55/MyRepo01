node {
	stage ('SCM checkout'){
		git "https://github.com/rkagr55/MyRepo01"
	}
	stage ('Build'){
    	    dir("Artifact001") {
	        bat "mvn -B package --file Artifact001/pom.xml clean install"
            }
       	    dir("Artifact001/target") {
	        sh "java -jar com.test-1.0-SNAPSHOT.jar"
            }
        }
}
